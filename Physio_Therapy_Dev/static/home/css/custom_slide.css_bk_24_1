/**************** for Drag And Drop Icon *****************/

.forDragAndDropIcon
{
    position: relative;

    min-height: 40px;
    padding-right: 50px;
}
.forDragAndDropIcon .curve
{
    position: absolute;
    top: -21px;
    right: -12%;

    width: 200px;
    height: 70px;

    content: '';
    -webkit-transition: all .3s ease;
       -moz-transition: all .3s ease;
        -ms-transition: all .3s ease;
            transition: all .3s ease;
    animation: rotateImg 3s infinite;

    background: url(../images/dragAndDrop.png) center center no-repeat;
    background-size: contain;
}
@keyframes rotateImg
{
    0%
    {
        transform: rotate(0deg);
    }

    50%
    {
        transform: rotate(-10deg);
    }

    100%
    {
        transform: rotate(0deg);
    }
}
@-webkit-keyframes rotateImg
{
    0%
    {
        transform: rotate(0deg);
    }

    50%
    {
        transform: rotate(-10deg);
    }

    100%
    {
        transform: rotate(0deg);
    }
}
@-moz-keyframes rotateImg
{
    0%
    {
        transform: rotate(0deg);
    }

    50%
    {
        transform: rotate(-10deg);
    }

    100%
    {
        transform: rotate(0deg);
    }
}
