/* *************************************************************
************************** Header ******************************
*************************************************************** */
.aa {
	background-color: rgba(255, 255, 255, 0.9);
	position: fixed;
	z-index: 999;
	width: 100%;
}
.headerBorder{
	border-bottom: 2px solid gainsboro;
	transition: all .3s ease;
}
.logo a {
	width: 13%;
	float: left;
	text-align: center;
}
.logo img{
	min-width: 65%;
}
.menu {
	width: 82%;
	float: left;
}
.menu ul {
	padding: 30px 15px;
}
.menu ul li {
	display: inline-block;
}
.menu ul li a {
	display: block;
    color: #000;
    font-size: 16px;
    padding: 10px 20px;
    transition: all .3s ease;
}
.menu ul li a:hover {
	color: #fff;
	background: #000;
}
.font-chat {
	width: 22%;
	float: left;
	
}
.font-chat i {
	font-size: 30px;
}
.chat-section {
	width: 75%;
	float: right;
	
}
.right-header {
	width: 80%;
	float: right;
}
.chat-icon {
	width: 16%;
	float: left;
	padding-top: 30px;
}
.chat-section h4 {
	font-size: 20px;
	color: #000;
	text-align: left;
	line-height: 10px;
}
.chat-section h5 {
	font-size: 26px;
	color: #000;
	font-weight: bold;
}
/* *************************************************************
************************** Banner ******************************
*************************************************************** */
.banner img {
	top: 0;
	position: absolute;
}
#banner {
	width: 100%;
	top: 0px;
	left: 0;
	border-bottom: 15px solid #191818;
}
#banner .banner_content .caption {
	position: absolute;
	width: 40%;
	height: 100%;
	background: rgba(0,0,0,.6);
	left: 58%;
	margin-left: -30%;
	padding: 20px 0;
	transform: skew(-15deg);
}
.caption-text {
	width: 100%;
	position: absolute;
	top: 300px;
}
#banner .banner_content .caption-text h1 {
	color: #fff;
	margin-top: 50px;
	font-size: 68px;
	font-weight: bold;
}
#banner .banner_content .caption-text h4 {
	color: #fff;
	font-size: 68px;
}
/* *************************************************************
************************** About Our Fleet ******************************
*************************************************************** */
#Fleet {
	width: 100%;
	padding: 60px 0;
}
#Fleet h3 {
	color: #000;
	padding-bottom: 90px;
	color: #2f2e2e;
}
#Fleet h4 {
	text-align: right;
	color: #292828;
	font-size: 34px;
}
#Fleet p {
	text-align: right;
}
.fleet-container1 {
	width: 100%;
	height: auto;
	padding-bottom: 55px;
}
.fleet-container2 {
	width: 100%;
	height: auto;
}
.fleet-container1 .fleet-img img{
	width: 95%;
}
.fleet-container1 .fleet-img {
	padding-bottom: 20px;
	width: 45%;
	float: left;
	position: relative;
	background: url('images/fleet-img_bg.png') no-repeat right bottom;
}
.fleet-container2 .fleet-img {
	width: 45%;
	float: right;
	padding-left: 20px;
	position: relative;
	padding-top: 20px;
	background: url('images/fleet-img_bg2.png') no-repeat top left;
}
.fleet-container1 .fleet-content {
	width: 53%;
	float: right;
	padding-top: 50px;
}
.fleet-container2 .fleet-content {
	width: 49%;
	float: left;
	padding-top: 50px;
}
/* *************************************************************
************************** Our Services ******************************
*************************************************************** */

#services {
	background: url(images/services_img.png);
	background-size: cover;
	background-position: center;
	width: 100%;
}
.services-cover {
	background-color: rgba(0,0,0,0.8);
	width: 100%;
	padding: 75px 0;
}
.services-cover h3 {
	color: #fff;
}
#w {
	display: block;
	max-width: 920px;
	min-width: 300px;
	margin: 0 auto;
	padding: 2px 3px;
	position: relative;
	margin-top: 35px;
}
.crsl-items {
	display: block;
	padding: 5px;
}
.crsl-item {
	background: #000;
	padding: 30px 8px;
	-webkit-box-shadow: 0 2px 3px rgba(0,0,0,0.4);
	-moz-box-shadow: 0 2px 3px rgba(0,0,0,0.4);
	box-shadow: 0 2px 3px rgba(0,0,0,0.4);
}
.crsl-item .thumbnail {
	display: block;
	position: relative;
	margin-bottom: 10px;
	cursor: pointer;
}
.crsl-item .thumbnail img {
	display: block; /* fix 1px image space http://stackoverflow.com/q/5804256/477958 */
	-webkit-transition: all 0.3s linear;
	-moz-transition: all 0.3s linear;
	transition: all 0.3s linear;
	margin: 0 auto;
}
.crsl-item .thumbnail:hover img {
	opacity: 0.8;
}
.crsl-item h3 {
	font-family: corbel;
	font-size: 1.95em;
	line-height: 1.25em;
	margin-bottom: 12px;
}
.crsl-item h3 a {
	text-decoration: none !important;
	color: #fff;
	font-size: 27px;
	font-family: corbel;
	text-transform: capitalize;
}
.crsl-item h3 a:hover {
	text-decoration: underline;
}
.crsl-item .postdate {
	display: block;
	position: absolute;
	bottom: 0;
	right: 0;
	padding: 6px;
	color: #fff;
	text-shadow: 1px 1px 0 rgba(0,0,0,0.4);
	font-size: 1.2em;
	font-weight: bold;
	background: #4e90da;
}
.crsl-item p {
	font-size: 18px;
	line-height: 1.55em;
	margin-bottom: 5px;
	color: #bababa;
}
.crsl-item p.readmore a {
	display: block;
	float: right;
	color: #4e90da;
	padding: 3px 5px;
	text-decoration: none;
	font-weight: bold;
	-webkit-border-radius: 3px;
	-moz-border-radius: 4px;
	border-radius: 4px;
}
.crsl-item p.readmore a:hover {
	background: #4b6caa;
	color: #fff;
}
.slidernav {
	display: block;
	text-align: center;
	margin-bottom: 5px;
}
#navbtns .previous {
	position: absolute;
	top: 152px;
	right: -80px;
}
#navbtns .next1 {
	position: absolute;
	top: 152px;
	left: -80px;
}
.slidernav a:active {
	-webkit-box-shadow: 1px 1px 0 rgba(0,0,0,0.2);
	-moz-box-shadow: 1px 1px 0 rgba(0,0,0,0.2);
	box-shadow: 1px 1px 0 rgba(0,0,0,0.2);
}
