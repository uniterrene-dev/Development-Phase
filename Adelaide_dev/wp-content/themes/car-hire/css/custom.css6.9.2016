@font-face {

	font-family: corbel;

	src: url(../fonts/CORBEL_0.ttf)

}

* {

	box-sizing: border-box

}

body {

	margin: 0;

	padding: 0 !important;

	font-family: corbel;

}

body, html {

	font-size: 16px;

}

div, table, tr, th, td, th, video, audio, h1, h2, h3, h4, h5, h6, aside, canvas, section, header, footer, nav, span, p, small, big, a, b, strong, img, hr, pre, button, input, textarea, optgroup, ul, li, ol, select, option, blockquote, nav {

	margin: 0;

	padding: 0;

	outline: none;

	border: none;

	text-decoration: none;

	list-style-type: none;

}

.clear{

	clear: both;

}

.clearfix:after {

	content: "";

	display: block;

	height: 0;

	clear: both;

	font-size: 0;

}

.clear {

	clear: both

}

.container {

	margin: 0 auto;

	padding: 0 0px;

}

.row {

	width: 100%;

	margin: 10px auto

}

img {

	max-width: 100%;

}

h1, h2, h3 {

	font-family: corbel;

	font-weight: normal;

	word-spacing: 10px

}

h4, h5, h6 {

	font-family: corbel;

}

h1 {

	font-size: 68px;

	text-align: center;

	font-weight: normal;

	text-transform: uppercase;

	font-weight: normal

}

h2 {

	font-size: 54px;

	text-align: center;

	line-height: 54px;

	font-weight: bold;

	text-transform: uppercase

}

h3 {

	font-size: 54px;

	line-height: 40px;

	text-align: center;

	font-weight: normal;

	text-transform: uppercase

}

h4 {

	font-size: 23px;

	line-height: 30px;

	text-align: center;

	font-weight: normal;

}

h5 {

	font-size: 18px;

}

h6 {

}

p {

	font-size: 17px;

	line-height: 28px;

	font-weight: normal;

	color: #575555;

	text-align: center;

	margin: 15px 0

}

a{

	text-decoration: none;

	color: #000;

}

