	display: none;

	z-index: 99;

	position: fixed;

	top: 20%;

	right: -165px;

	background: rgba(255,0,0, .8);

	padding: 10px 12px;

	font-size: 30px;

	cursor: pointer;

	border-radius: 5px 0 0 5px;

	width: 215px;

	transition: all .5s ease;

}

.fixedChat:hover{

	right: 0;

}

