.menuForResponsive{

	position: absolute;

	top: 0;

	left: 0;

	right: 0;

	display: none;

}

.menuForResponsive ul{

	width: 50%;

	margin: 0 auto;

	text-align: center;

}

.menuForResponsive ul li a{

	display: block;

	padding: 10px;

	border-bottom: 1px solid gainsboro;

	background: rgba(255,255,255, .8);

}

