.f_left {

	float: left;

}

.f_right {

	float: right;

}

a.btn_bor {

	background: transparent;

	color: #ffffff;

	text-align: center;

	line-height: 45px;

	font-size: 18px;

	transition: all .3s ease;

	-moz-transition: all .3s ease;

	-webkit-transition: all .3s ease;

